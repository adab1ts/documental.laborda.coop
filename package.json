{
  "name": "laborda.coop",
  "version": "1.0.0",
  "description": "Web documental de La Borda",
  "keywords": [
    "semantic-ui",
    "es2015",
    "babel",
    "webpack"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://documental.laborda.coop",
  "repository": "github:adab1ts/documental.laborda.coop",
  "author": {
    "name": "Carles Mui√±os",
    "email": "carles@adabits.org"
  },
  "private": true,
  "scripts": {
    "postinstall": "node config/semantic-fix.js",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules",
    "lint:scripts": "snazzy --verbose",
    "lint:styles": "stylelint 'src/**/*.css' 'vendor/**/*.less' 'vendor/**/*.variables' 'vendor/**/*.overrides' -q",
    "lint": "run-p lint:*",
    "favicon": "cpy favicon.ico dist",
    "prebuild:dev": "run-s clean lint favicon",
    "build:dev": "webpack --display-modules --display-chunks --watch --env.dev",
    "serve:dev": "lite-server -c config/lite-server.js",
    "dev": "run-p build:dev serve:dev",
    "prebuild:prod": "run-s clean lint favicon",
    "build:prod": "webpack --display-modules --display-chunks --bail --env.prod",
    "preprod": "npm run build:prod",
    "prod": "http-server dist -g --cors",
    "start": "npm run dev"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "open-color": "1.4.0",
    "semantic-ui-less": "^2.2.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "compression-webpack-plugin": "^0.3.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "image-webpack-loader": "3.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lite-server": "^2.2.2",
    "npm-run-all": "^3.1.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-loader": "1.2.1",
    "progress-bar-webpack-plugin": "1.9.1",
    "rimraf": "^2.5.4",
    "snazzy": "^5.0.0",
    "standard": "^8.0.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.3.1",
    "stylelint-config-standard": "^13.0.2",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.2",
    "webpack-merge": "2.0.0"
  }
}
